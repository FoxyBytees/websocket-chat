{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "cargo build --bin client --package client",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "cargo build client",
			"options": {
                "cwd": "${workspaceFolder}\\client",
            }
		},
		{
			"type": "shell",
			"command": "cargo build --bin server --package server",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "cargo build server",
			"options": {
                "cwd": "${workspaceFolder}\\server",
            }
		}
	]
}